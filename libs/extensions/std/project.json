{
  "name": "extensions-std",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/extensions/std/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/extensions/std",
        "main": "libs/extensions/std/src/index.ts",
        "tsConfig": "libs/extensions/std/tsconfig.lib.json",
        "assets": ["libs/extensions/std/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/extensions/std/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/extensions/std/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
